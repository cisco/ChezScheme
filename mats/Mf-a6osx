m = a6osx

fsrc = foreign1.c foreign2.c foreign3.c
fobj = foreign1.so

include Mf-base

foreign1.so: ${fsrc} ../boot/$m/scheme.h
	cc -m64 -dynamiclib -undefined dynamic_lookup -I${Include} -o foreign1.so ${fsrc}

cat_flush: cat_flush.c
	cc -o cat_flush cat_flush.c
