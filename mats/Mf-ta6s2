m = ta6s2

fsrc = foreign1.c foreign2.c foreign3.c foreign4.c
fobj = foreign1.so

include Mf-base

foreign1.so: ${fsrc} ../boot/$m/scheme.h
	gcc -m64 -D_REENTRANT -fPIC -shared -I${Include} -o foreign1.so ${fsrc}

cat_flush: cat_flush.c
	gcc -o cat_flush cat_flush.c
